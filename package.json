{
    "name": "ui",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "host": "ng serve --host 0.0.0.0",
        "build": "ng build --configuration development",
        "build-prod": "ng build --base-href /",
        "build-live": "ng build --watch --configuration=app",
        "watch": "ng build --watch --configuration development",
        "platform:add:android": "npx cap add android",
        "platform:add:ios": "npx cap add ios",
        "build-android": "npm run build:app:android:watch:dev && npm run version-android",
        "build:app:android:watch:dev": "npm run resources && ng build --configuration=app && npx cap sync android && npx cap copy android && npx cap open android",
        "build:app:ios:watch:dev": "ng build --configuration=app && npx cap copy ios && npx cap open ios",
        "build-in-ios": "rm -Rf ios && ng build --configuration=app && npx cap add ios && npx cap sync ios && node resources-ios.js && npx cap open ios",
        "test": "ng test",
        "lint": "npx eslint src/**/*.{js,jsx,ts,tsx,html} --quiet --fix",
        "format": "npx prettier src/**/*.{js,jsx,ts,tsx,html} --write",
        "resources": "cordova-res android --skip-config --copy && node resources.js",
        "version-android": "capacitor-set-version set:android -b 1 -v"
    },
    "private": true,
    "dependencies": {
        "@adyen/adyen-web": "^5.13.1",
        "@angular/animations": "~13.2.0",
        "@angular/cdk": "^13.3.2",
        "@angular/common": "~13.2.0",
        "@angular/compiler": "~13.2.0",
        "@angular/core": "~13.2.0",
        "@angular/forms": "~13.2.0",
        "@angular/material": "^13.3.2",
        "@angular/platform-browser": "~13.2.0",
        "@angular/platform-browser-dynamic": "~13.2.0",
        "@angular/router": "~13.2.0",
        "@angular/service-worker": "~13.2.0",
        "@capacitor/android": "^3.5.0",
        "@capacitor/app": "^1.1.1",
        "@capacitor/core": "^3.5.0",
        "@capacitor/ios": "^3.5.0",
        "@capacitor/status-bar": "^1.0.8",
        "@gameleap/capacitor-chromecast": "file:./plugins/capacitor-chromecast-master",
        "@hugotomazi/capacitor-navigation-bar": "^2.0.0",
        "@robingenz/capacitor-screen-orientation": "^1.1.7",
        "@schematics/angular": "^15.2.0",
        "capacitor-set-version": "^2.0.3",
        "cordova-plugin-screen-orientation": "^3.0.2",
        "es6-promise-plugin": "^4.2.2",
        "ng-drag-drop": "^5.0.0",
        "ngx-cookie-service": "^13.1.2",
        "ngx-device-detector": "^3.0.0",
        "ngx-infinite-scroll": "^13.0.1",
        "ngx-mask": "^13.1.14",
        "ngx-skeleton-loader": "^5.0.0",
        "rxjs": "~7.5.0",
        "swiper": "^8.0.6",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/architect": "^0.1302.4",
        "@angular-devkit/build-angular": "~13.2.3",
        "@angular-devkit/build-webpack": "^0.1302.4",
        "@angular-devkit/schematics": "^15.2.0",
        "@angular-eslint/builder": "13.1.0",
        "@angular-eslint/eslint-plugin": "13.1.0",
        "@angular-eslint/eslint-plugin-template": "13.1.0",
        "@angular-eslint/schematics": "13.1.0",
        "@angular-eslint/template-parser": "13.1.0",
        "@angular/cli": "^15.2.0",
        "@angular/compiler-cli": "~13.2.0",
        "@capacitor/cli": "^3.5.0",
        "@types/jasmine": "~3.10.0",
        "@types/node": "^17.0.19",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jasmine-core": "~4.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "prettier": "^2.5.1",
        "typescript": "~4.5.2"
    }
}
